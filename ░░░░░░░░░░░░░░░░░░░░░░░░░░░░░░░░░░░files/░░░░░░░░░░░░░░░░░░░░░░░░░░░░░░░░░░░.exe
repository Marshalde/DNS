// Define a class for managing a user's profile
class UserProfile {
  constructor(username, email) {
    this.username = username;
    this.email = email;
    this.posts = [];
  }

  // Add a new post to the user's profile
  addPost(title, content) {
    const post = new Post(title, content);
    this.posts.push(post);
  }

  // Display all posts in the user's profile
  displayPosts() {
    console.log(`Posts by ${this.username}:`);
    this.posts.forEach((post, index) => {
      console.log(`Post ${index + 1}: ${post.title}`);
      console.log(post.content);
    });
  }
}

// Define a class for individual posts
class Post {
  constructor(title, content) {
    this.title = title;
    this.content = content;
    this.comments = [];
  }

  // Add a comment to the post
  addComment(author, text) {
    const comment = new Comment(author, text);
    this.comments.push(comment);
  }
}

// Define a class for comments
class Comment {
  constructor(author, text) {
    this.author = author;
    this.text = text;
  }
}

// Create a user profile and add posts and comments
const user = new UserProfile("jsCoder", "js@example.com");
user.addPost("My First Post", "This is the content of my first post.");
user.addPost("Another Post", "Here's another interesting post.");
user.posts[0].addComment("user123", "Nice post!");
user.posts[0].addComment("user456", "Great job!");

// Display the user's profile and posts
user.displayPosts();



